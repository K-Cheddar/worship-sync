  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:lint": "npm run lint && vite build",
    "build:strict": "npm run lint:check && vite build",
    "build:electron": "vite build --config vite.config.electron.ts && ELECTRON_BUILD=true vite build",
    "electron": "npm run build:electron:files && wait-on https://local.worshipsync.net:3000 && electron .",
    "electron:dev": "concurrently \"npm run dev\" \"npm run build:electron:files && wait-on https://local.worshipsync.net:3000 && electron .\"",
    "build:electron:files": "vite build --config vite.config.electron.ts",
    "electron:build": "npm run build:electron && electron-builder",
    "electron:build:win": "npm run build:electron && electron-builder --win",
    "electron:build:mac": "npm run build:electron && electron-builder --mac",
    "electron:build:linux": "npm run build:electron && electron-builder --linux",
    "test": "vite test",
    "eject": "vite eject",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint src --ext .ts,.tsx,.js,.jsx --fix",
    "lint:check": "eslint src --ext .ts,.tsx,.js,.jsx --max-warnings 0",
    "type-check": "tsc --noEmit"
  },
